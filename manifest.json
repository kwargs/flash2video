{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "replacer.js" ],
      "matches": [ "http://*/*" ],
      "run_at": "document_end"
   } ],

   "description": "Automatically replaces embedded *tube flash player to html5 player",
   "icons": {
      "128": "images/icon.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "permisions": ["http://*/*"],

   "name": "Flash to Video",
   "version": "0.1"
}
